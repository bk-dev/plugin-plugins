    "@types/supports-color": "^3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.16.0",
    "eslint-config-anycli": "^1.3.1",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/anycli/plugin-plugins",
  "keywords": [
    "anycli-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "anycli/plugin-plugins",
  "scripts": {
    "build": "rm -rf lib && tsc",
