  "dependencies": {
    "@anycli/command": "^0.3.6",
    "@anycli/manifest-file": "^0.3.8",
    "@heroku-cli/color": "^1.1.3",
    "chalk": "^2.3.0",
    "cli-ux": "^3.3.12",
    "debug": "^3.1.0",
    "fs-extra": "^5.0.0",
    "http-call": "^5.0.2",
    "lodash": "^4.17.4",
    "npm-run-path": "^2.0.2",
    "tslib": "^1.9.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "@anycli/config": "^0.2.11",
    "@anycli/engine": "^0.2.0",
    "@anycli/plugin-help": "^0.4.2",
    "@anycli/plugin-version": "^0.1.20",
    "@anycli/test": "^0.10.3",
    "@anycli/tslint": "^0.2.2",
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^5.0.0",
